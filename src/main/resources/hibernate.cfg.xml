<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<!-- specify jdbc connection -->
		<property name="connection.driver_class">com.mysql.jdbc.Driver</property>
<!--		<property name="connection.url">jdbc:mysql://localhost:3306/realestate?serverTimezone=UTC</property>-->
		<property name="connection.url">jdbc:mysql://localhost:3306/realestateanalyser?serverTimezone=UTC</property>
		<property name="connection.username">root</property>
<!--		<property name="connection.password">installSQLpassword</property>-->
		<property name="connection.password">Password@17</property>
	</session-factory>
</hibernate-configuration>